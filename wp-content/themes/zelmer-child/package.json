    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "@wordpress/api-fetch": "^5.2.6",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.0",
    "formbouncerjs": "^1.4.6",
    "jquery": "^3.6.0",
    "panelsnap": "^1.3.0",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "stylelint": "^14.1.0",
