{
  "name": "zelmer-wordpress-theme",
  "version": "1.0.0",
  "description": "Custom Wordpress Theme",
  "main": "postcss.config.js",
  "scripts": {
    "serve": "webpack-dev-server",
    "start": "webpack --config webpack.dev.js",
    "build": "rimraf assets && webpack --progress --config webpack.prod.js",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:scripts": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx \"src\"",
    "fix:html": "prettier \"src/**/*.html\" --write",
    "fix:styles": "stylelint \"src/**/*.scss\" --fix",
    "fix:scripts": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx \"src\" --fix",
    "webp": "node webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://PiotrekNiemirski@bitbucket.org/theypl/zelmer_website_wordpress.git"
  },
  "author": "they.pl",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/theypl/zelmer_website_wordpress/issues"
  },
  "homepage": "https://bitbucket.org/theypl/zelmer_website_wordpress#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@babel/runtime-corejs2": "^7.16.3",
    "@types/bootstrap": "^5.1.6",
    "@types/jquery": "^3.5.9",
    "@types/vimeo__player": "^2.10.1",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.2",
    "@types/youtube-player": "^5.5.5",
    "@types/yt-player": "^3.5.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@webpack-cli/init": "^1.1.3",
    "@webpack-cli/serve": "^1.6.0",
    "@wordpress/scripts": "^19.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dir-archiver": "^1.2.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
