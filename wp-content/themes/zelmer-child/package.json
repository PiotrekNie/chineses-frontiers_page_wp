    "youtube-player": "^5.5.2",
    "yt-player": "^3.5.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.tsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "src/**/*.scss": [
      "npm run fix:styles"
    ]
  }
}
